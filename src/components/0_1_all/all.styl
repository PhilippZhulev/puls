body
    font-family 'Roboto'
    font-weight 400

.container
    max-width 1530px
    width 100%
    position relative
section
    width 100%
    height 1080px
    background #f9f5f7
    position relative
    overflow hidden
    .container
        padding-top 176px
        padding-bottom 40px
    &.layout-1
        z-index 10
    &.layout-2
        z-index 20
    &.layout-3
        z-index 30
        background #fff
@media all and (max-width 1200px)
    section
        height auto
    .container
        padding 0 30px
